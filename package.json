{
  "name": "@hyperoracle/release-scripts",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.15.4",
  "description": "",
  "author": "MuRong <hi@mrong.me>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/murongg",
  "homepage": "https://github.com/murongg/@hyperoracle/release-scripts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/murongg/@hyperoracle/release-scripts.git"
  },
  "bugs": {
    "url": "https://github.com/murongg/@hyperoracle/release-scripts/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "scripts": "bin/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "bumpp": "^9.3.0",
    "cac": "^6.7.14",
    "fs-extra": "^11.2.0",
    "mri": "^1.2.0",
    "picocolors": "^1.0.0",
    "semver": "^7.6.0",
    "unbuild": "^2.0.0",
    "unconfig": "^0.3.11"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@murongg/eslint-config": "^0.2.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^18.19.18",
    "@types/semver": "^7.5.8",
    "eslint": "^8.57.0",
    "esno": "^4.0.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^8.15.4",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
